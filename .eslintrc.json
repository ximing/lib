{
  "extends": ["standard", "standard-jsx", "plugin:prettier/recommended"],
  "plugins": ["jsx-a11y"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-pascal-case": "off",
    "no-unused-vars": "off",
    "comma-dangle": "off",
    "@typescript-eslint/no-unused-vars": [
      "off",
      {
        "ignoreRestSiblings": true
      }
    ]
  }
}
